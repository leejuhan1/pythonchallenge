################ before ################

import string

mission = "map"
result = ""

for i in mission:
  if i in string.ascii_lowercase:
    word = ord(i)+2
    if word > ord("z"):
      word = word - 26
    result += chr(word)

  else:
    result += " "

print(result)



################ after ################

import string
 
word = "map"
 
table = str.maketrans(string.ascii_lowercase, "cdefghijklmnopqrstuvwxyzab")
print(word.translate(table))
